<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_layout.xsd">
    <update handle="1column"/>
    <referenceContainer name="content">
        <container name="custom.inner.content" htmlTag="div" htmlClass="container">
            <container name="custom.inner.wrapper" htmlTag="div" htmlClass="cms-wrap" />
        </container>
    </referenceContainer>
    <move element="cms_page" destination="custom.inner.wrapper" />
</layout>
