<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <head>
        <title>SlideTeam</title>
    </head>
    <body>
        <referenceBlock name="custom.page.title">
            <action method="setPageTitle">
                <argument name="title" translate="true" xsi:type="string">You are now logged out</argument>
            </action>
        </referenceBlock>
        <move element="customer_logout" destination="custom.account.wrapper" />
    </body>
</page>
