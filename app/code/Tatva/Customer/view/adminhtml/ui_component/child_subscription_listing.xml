<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">child_subscription_listing.child_subscription_form_data_source</item>
		</item>
	</argument>
	<settings>
		<spinner>child_subscription_listing_columns</spinner>
		<deps>
			<dep>child_subscription_listing.child_subscription_form_data_source</dep>
		</deps>
	</settings>
	<dataSource name="child_subscription_form_data_source" component="Magento_Ui/js/grid/provider">
		<argument name="dataProvider" xsi:type="configurableObject">
			<argument name="class" xsi:type="string">Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider</argument>
			<argument name="name" xsi:type="string">child_subscription_form_data_source</argument>
			<argument name="primaryFieldName" xsi:type="string">subscription_history_id</argument>
			<argument name="requestFieldName" xsi:type="string">subscription_history_id</argument>
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="component" xsi:type="string">Magento_Ui/js/grid/provider</item>
					<item name="update_url" xsi:type="url" path="mui/index/render"/>
					<item name="storageConfig" xsi:type="array">
						<item name="indexField" xsi:type="string">subscription_history_id</item>
					</item>
				</item>
			</argument>
		</argument>
	</dataSource>

	<listingToolbar name="listing_top">
		<settings>
			<sticky>false</sticky>
		</settings>
		<bookmark name="bookmarks">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="storageConfig" xsi:type="array">
						<item name="namespace" xsi:type="string">child_subscription_listing</item>
					</item>
				</item>
			</argument>
		</bookmark>
		<component name="columns_controls">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="columnsData" xsi:type="array">
						<item name="provider" xsi:type="string">child_subscription_listing.child_subscription_listing.child_subscription_listing_columns</item>
					</item>
					<item name="component" xsi:type="string">Magento_Ui/js/grid/controls/columns</item>
					<item name="displayArea" xsi:type="string">dataGridActions</item>
				</item>
			</argument>
		</component>
		
		<filters name="listing_filters">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="storageConfig" xsi:type="array">
						<item name="provider" xsi:type="string">child_subscription_listing.child_subscription_listing.listing_top.bookmarks</item>
						<item name="namespace" xsi:type="string">current.filters</item>
					</item>
					<item name="templates" xsi:type="array">
						<item name="filters" xsi:type="array">
							<item name="select" xsi:type="array">
								<item name="component" xsi:type="string">Magento_Ui/js/form/element/ui-select</item>
								<item name="template" xsi:type="string">ui/grid/filters/elements/ui-select</item>
							</item>
						</item>
					</item>
					<item name="childDefaults" xsi:type="array">
						<item name="provider" xsi:type="string">child_subscription_listing.child_subscription_listing.listing_top.listing_filters</item>
						<item name="imports" xsi:type="array">
							<item name="visible" xsi:type="string">child_subscription_listing.child_subscription_listing.child_subscription_listing_columns.${ $.index }:visible</item>
						</item>
					</item>
				</item>
				<item name="observers" xsi:type="array">
					<item name="column" xsi:type="string">column</item>
				</item>
			</argument>
		</filters>
		<paging name="listing_paging">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="storageConfig" xsi:type="array">
						<item name="provider" xsi:type="string">child_subscription_listing.child_subscription_listing.listing_top.bookmarks</item>
						<item name="namespace" xsi:type="string">current.paging</item>
					</item>
					<item name="selectProvider" xsi:type="string">child_subscription_listing.child_subscription_listing.cms_block_columns.ids</item>
				</item>
			</argument>
		</paging>
	</listingToolbar>

	<columns name="child_subscription_listing_columns">
		<column name="email">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="filter" xsi:type="string">text</item>
					<item name="editor" xsi:type="array">
						<item name="editorType" xsi:type="string">text</item>
						<item name="validation" xsi:type="array">
							<item name="required-entry" xsi:type="boolean">true</item>
						</item>
					</item>
					<item name="label" xsi:type="string" translate="true">customer email</item>
				</item>
			</argument>
		</column>
		<column name="subscription_period">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="filter" xsi:type="string">text</item>
					<item name="editor" xsi:type="array">
						<item name="editorType" xsi:type="string">text</item>
						<item name="validation" xsi:type="array">
							<item name="required-entry" xsi:type="boolean">true</item>
						</item>
					</item>
					<item name="label" xsi:type="string" translate="true">subscription period</item>
				</item>
			</argument>
		</column>
		<column name="from_date">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="filter" xsi:type="string">dateRange</item>
					<item name="editor" xsi:type="array">
						<item name="editorType" xsi:type="string">text</item>
						<item name="validation" xsi:type="array">
							<item name="required-entry" xsi:type="boolean">true</item>
						</item>
					</item>
					<item name="label" xsi:type="string" translate="true">From Date</item>
				</item>
			</argument>
		</column>
		<column name="to_date">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="filter" xsi:type="string">dateRange</item>
					<item name="editor" xsi:type="array">
						<item name="editorType" xsi:type="string">text</item>
						<item name="validation" xsi:type="array">
							<item name="required-entry" xsi:type="boolean">true</item>
						</item>
					</item>
					<item name="label" xsi:type="string" translate="true">To Date</item>
				</item>
			</argument>
		</column>
		<column name="renew_date">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="filter" xsi:type="string">dateRange</item>
					<item name="editor" xsi:type="array">
						<item name="editorType" xsi:type="string">text</item>
						<item name="validation" xsi:type="array">
							<item name="required-entry" xsi:type="boolean">true</item>
						</item>
					</item>
					<item name="label" xsi:type="string" translate="true">Renew Date</item>
				</item>
			</argument>
		</column>
		<column name="download_limit">
			<argument name="data" xsi:type="array">
				<item name="options" xsi:type="object">Tatva\Customer\Ui\Component\Listing\Column\DeleteChildsubscriberStatus</item>
				<item name="config" xsi:type="array">
					<item name="filter" xsi:type="string">select</item>
					<item name="label" xsi:type="string" translate="true">Status</item>
					<item name="component" xsi:type="string">Magento_Ui/js/grid/columns/select</item>
	                <item name="dataType" xsi:type="string">select</item>
	                <item name="editor" xsi:type="array">
	                    <item name="editorType" xsi:type="string">select</item>
	                </item>
				</item>
			</argument>
		</column>		
	</columns>
</listing>