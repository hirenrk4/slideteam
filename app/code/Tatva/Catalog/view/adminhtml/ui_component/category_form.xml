<?xml version="1.0" encoding="UTF-8"?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <fieldset name="general_tab">
        <argument name="data" xsi:type="array">
            <item name="config" xsi:type="array">
                <item name="label" xsi:type="string" translate="true">General</item>
                <item name="collapsible" xsi:type="boolean">true</item>
                <item name="sortOrder" xsi:type="number">15</item>
            </item>
        </argument>
        <field name="visual_search">
            <argument name="data" xsi:type="array">
                <item name="options" xsi:type="object">Magento\Config\Model\Config\Source\Yesno</item>
                <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">10</item>
                    <item name="formElement" xsi:type="string">select</item>
                    <item name="scopeLabel" xsi:type="string" translate="true">[GLOBAL VIEW]</item>
                    <item name="dataType" xsi:type="string">select</item>
                    <item name="label" xsi:type="string" translate="true">Visual Search Category</item>
                    <item name="source" xsi:type="string">custom_attribute_new</item>
                    <item name="caption" xsi:type="string" translate="true">-- Please Select --</item>
                </item>
            </argument>
        </field>
        <field name="sub_category_id">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">20</item>
                    <item name="dataType" xsi:type="string">string</item>
                    <item name="formElement" xsi:type="string">input</item>
                    <item name="scopeLabel" xsi:type="string" translate="true">[GLOBAL VIEW]</item>
                    <item name="label" xsi:type="string" translate="true">Sub Category ID</item>
                    <item name="source" xsi:type="string">sub_category_id</item>
                </item>
            </argument>
        </field>
        <field name="category_header">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">30</item>
                    <item name="dataType" xsi:type="string">string</item>
                    <item name="formElement" xsi:type="string">input</item>
                    <item name="scopeLabel" xsi:type="string" translate="true">[GLOBAL VIEW]</item>
                    <item name="label" xsi:type="string" translate="true">Category Title</item>
                    <item name="source" xsi:type="string">category_header</item>
                </item>
            </argument>
        </field>
        <field name="complete_decks">
            <argument name="data" xsi:type="array">
                <item name="options" xsi:type="object">Magento\Config\Model\Config\Source\Yesno</item>
                <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">40</item>
                    <item name="dataType" xsi:type="string">select</item>
                    <item name="formElement" xsi:type="string">select</item>
                    <item name="scopeLabel" xsi:type="string" translate="true">[GLOBAL VIEW]</item>
                    <item name="label" xsi:type="string" translate="true">Complete Decks Category</item>
                    <item name="source" xsi:type="string">complete_decks</item>
                    <item name="caption" xsi:type="string" translate="true">-- Please Select --</item>
                </item>
            </argument>
        </field>
        <field name="move_to_other_category">
            <argument name="data" xsi:type="array">
                <item name="options" xsi:type="object">Magento\Config\Model\Config\Source\Yesno</item>
                <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">50</item>
                    <item name="dataType" xsi:type="string">select</item>
                    <item name="formElement" xsi:type="string">select</item>
                    <item name="scopeLabel" xsi:type="string" translate="true">[GLOBAL VIEW]</item>
                    <item name="label" xsi:type="string" translate="true">Show in Other Category instead of individual</item>
                    <item name="source" xsi:type="string">move_to_other_category</item>
                    <item name="caption" xsi:type="string" translate="true">-- Please Select --</item>
                </item>
            </argument>
        </field>
        <field name="seo_description">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">60</item>
                    <item name="dataType" xsi:type="string">string</item>
                    <item name="formElement" xsi:type="string">textarea</item>
                    <item name="scopeLabel" xsi:type="string" translate="true">[GLOBAL VIEW]</item>
                    <item name="label" xsi:type="string" translate="true">Category Description</item>
                    <item name="source" xsi:type="string">seo_description</item>
                </item>
            </argument>
        </field>
        <field name="show_in_frontend_listing">
            <argument name="data" xsi:type="array">
                <item name="options" xsi:type="object">Magento\Config\Model\Config\Source\Yesno</item>
                <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">70</item>
                    <item name="dataType" xsi:type="string">select</item>
                    <item name="formElement" xsi:type="string">select</item>
                    <item name="scopeLabel" xsi:type="string" translate="true">[GLOBAL VIEW]</item>
                    <item name="label" xsi:type="string" translate="true">Show In Frontend Listing</item>
                    <item name="source" xsi:type="string">show_in_frontend_listing</item>
                    <item name="caption" xsi:type="string" translate="true">-- Please Select --</item>
                </item>
            </argument>
        </field>
        <field name="seo_url">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">80</item>
                    <item name="dataType" xsi:type="string">string</item>
                    <item name="formElement" xsi:type="string">input</item>
                    <item name="scopeLabel" xsi:type="string" translate="true">[GLOBAL VIEW]</item>
                    <item name="label" xsi:type="string" translate="true">SEO URL</item>
                    <item name="source" xsi:type="string">seo_url</item>
                </item>
            </argument>
        </field>
    </fieldset>
</form>